spawn vim -N --noplugin -u ../vimrc
set timeout 3
expect {
  timeout {exit 1}
  "VIM - Vi IMproved"
}
send ":LustyFilesystemExplorer\r"
expect ">>"
send "random-file/"
# Should not have caused a backtrace and "Press ENTER ..." message
expect {
  "Press ENTER"  {exit 1}
  timeout {exit 1}
  "NO MATCHES"  # Success
}
# Ctrl-C
send \003
send \003
send \003
#send ":exec CheckForError()\r"
send ":q\r"
expect eof
exit 0

